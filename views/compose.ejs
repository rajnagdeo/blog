<%- include('header'); -%>
    <h1>Compose</h1>
    <form action="/compose" method="post">
        <div class="mb-3">
            <label  class="form-label ">Title</label>
            <input type="text" class="form-control inp" name="postTitle" id="">
        </div>
        <div class="mb-3">
            <label  class="form-label ">Content</label>
            <textarea class="form-control inp" id="" rows="3" name="postBody"></textarea>
        </div>
        <button type="submit" class="btn btn-outline-primary">Publish</button>
    
    </form>
<%- include('footer'); -%>